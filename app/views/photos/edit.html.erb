<div class="container_fuluid other-images" style="width: 100%; min-height: 600px;">
  <div class="beige-box">
<h1>写真編集画面</h1>
<%= form_with model: @photo do |f| %>
  <div>
    <p>タイトル</p>
    <%= f.text_field :title %>
    <p>コメント</p>
    <%= f.text_area :body %>
    <p>写真</p>
    <%= f.file_field :image, accept: "image/*" %>
    <%= f.submit "変更する" %>
  </div>
<% end %>
</div>
</div>